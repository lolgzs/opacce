msgid ""
"'%hostname%' does not appear to have a valid MX record for the email address "
"'%value%'"
msgstr ""
"'%hostname%' nu apare cu o înregistrare MX validă pentru adresa de email "
"'%valoare%'"

msgid "'%hostname%' is not a valid hostname for email address '%value%'"
msgstr ""
"'%hostname%' nu este un hostname valid pentru adresa de email '%valoare%'"

# nu sunt sigura de cuvantul rezolvata
msgid ""
"'%hostname%' is not in a routable network segment. The email address '%value"
"%' should not be resolved from public network."
msgstr ""
"'%hostname%' nu este un segment de reţea rutabil. Adresa email '%valoare%' "
"nu trebuie rezolvată din reţeaua publică. "

msgid "'%localPart%' can not be matched against dot-atom format"
msgstr "'%localPart%' nu poate fi comparat cu formatul dot-atom "

msgid "'%localPart%' can not be matched against quoted-string format"
msgstr "'%localPart%' nu poate fi comparat cu formatul quoted-string "

msgid "'%localPart%' is no valid local part for email address '%value%'"
msgstr ""
"'%localPart%' nu este o parte locală validă pentru adresa de email '%valoare"
"%'"

msgid "'%value%' appears to be a DNS hostname but cannot extract TLD part"
msgstr ""
"'%localPart%' pare a fi un hostname DNS dar nu se poate extrage rădăcina TLD"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match TLD against known "
"list"
msgstr ""
"'%valoare%' pare a fi un hostname DNS dar nu s-a găsit rădăcina TLD în lista "
"rădăcinilor cunoscute"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match against hostname "
"schema for TLD '%tld%'"
msgstr ""
"'%valoare%' pare a fi un hostname DNS dar nu poate fi comparat cu schema "
"hostname pentru TLD '%tld%'"

msgid ""
"'%value%' appears to be a DNS hostname but contains a dash in an invalid "
"position"
msgstr ""
"'%valoare%' pare a fi un hostname DNS dar conţine o liniuţă într-o poziţie "
"invalidă"

msgid ""
"'%value%' appears to be a DNS hostname but the given punycode notation "
"cannot be decoded"
msgstr ""
"'%valoare%' pare a fi un hostname DNS dar eticheta punycode dată nu poate fi "
"decodată "

msgid ""
"'%value%' appears to be a local network name but local network names are not "
"allowed"
msgstr ""
"'%valoare%' pare a fi o reţea locală dar numele de reţele locale nu sunt "
"permise"

msgid "'%value%' appears to be an IP address, but IP addresses are not allowed"
msgstr "'%valoare%' pare a fi o adresă IP, dar adresele IP nu sunt permise"

msgid "'%value%' does not appear to be a valid local network name"
msgstr "'%valoare%' nu pare un nume de reţea locală valid "

msgid "'%value%' does not appear to be an integer"
msgstr "'%valoare%' nu pare a fi un număr întreg"

msgid "'%value%' does not match the expected structure for a DNS hostname"
msgstr "'%valoare%' nu corespunde structurii obişnuite a unui hostname DNS "

msgid "'%value%' exceeds the allowed length"
msgstr "'%valoare%'depăşeşte lungimea permisă "

msgid "'%value%' is greater than %max% characters long"
msgstr "'%valoare%' este mai mare decât %max% caractere"

msgid "'%value%' is less than %min% characters long"
msgstr " '%valoare%' este mai mică decât %max% caractere"

msgid ""
"'%value%' is not a valid email address in the basic format local-"
"part@hostname"
msgstr ""
"'%valoare%' nu este o adresă de email valabilă în formatul de bază local-"
"part@hostname"

