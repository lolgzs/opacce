msgid ""
"'%hostname%' does not appear to have a valid MX record for the email address "
"'%value%'"
msgstr ""
"'%hostname%' ne semble pas avoir d'enregistrement MX valide pour l'adresse "
"email '%value%'"

msgid "'%hostname%' is not a valid hostname for email address '%value%'"
msgstr ""
"'%hostname%' n'est pas un nom d'hôte valide pour l'adresse email '%value%'"

msgid ""
"'%hostname%' is not in a routable network segment. The email address '%value"
"%' should not be resolved from public network."
msgstr ""
"'%hostname%' n'est pas dans un segment réseau routable. L'adresse email "
"'%value%' ne devrait pas être résolue depuis un réseau public."

msgid "'%localPart%' can not be matched against dot-atom format"
msgstr "'%localPart%' ne correspond pas au format dot-atom"

msgid "'%localPart%' can not be matched against quoted-string format"
msgstr "'%localPart%' ne correspond pas au format quoted-string"

msgid "'%localPart%' is no valid local part for email address '%value%'"
msgstr ""
"'%localPart%' n'est pas une partie locale valide pour l'adresse email '%value"
"%'"

msgid "'%value%' appears to be a DNS hostname but cannot extract TLD part"
msgstr ""
"'%value%' semble être un nom d'hôte DNS mais l'extension TLD ne peut être "
"extraite"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match TLD against known "
"list"
msgstr ""
"'%value%' semble être un nom d'hôte DNS mais son extension TLD semble "
"inconnue"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match against hostname "
"schema for TLD '%tld%'"
msgstr ""
"'%value%' semble être un nom d'hôte DNS valide mais ne correspond pas au "
"schéma de l'extension TLD '%tld%'"

msgid ""
"'%value%' appears to be a DNS hostname but contains a dash in an invalid "
"position"
msgstr ""
"'%value%' semble être un nom d'hôte DNS mais il contient un tiret à une "
"position invalide"

msgid ""
"'%value%' appears to be a DNS hostname but the given punycode notation "
"cannot be decoded"
msgstr "'%value%' semble être un DNS valide mais le code n'a pu être décodé"

msgid ""
"'%value%' appears to be a local network name but local network names are not "
"allowed"
msgstr ""
"'%value%' semble être un nom réseau local mais les noms locaux sont interdits"

msgid "'%value%' appears to be an IP address, but IP addresses are not allowed"
msgstr ""
"'%value%' semble être une IP valide mais celles-ci ne sont pas autorisées"

msgid "'%value%' does not appear to be a valid local network name"
msgstr "'%value%' ne semble pas être une adresse réseau local valide"

msgid "'%value%' does not appear to be an integer"
msgstr "'%value%' n'est pas un nombre entier"

msgid "'%value%' does not match the expected structure for a DNS hostname"
msgstr "'%value%' ne correspond pas à la structure d'un nom d'hôte DNS valide"

msgid "'%value%' exceeds the allowed length"
msgstr "'%value%' excède la taille autorisée"

msgid "'%value%' is greater than %max% characters long"
msgstr "'%value%' contient plus de %max% caractères"

msgid "'%value%' is less than %min% characters long"
msgstr "'%value%' contient moins de %min% caractères"

msgid ""
"'%value%' is not a valid email address in the basic format local-"
"part@hostname"
msgstr "'%value%' n'est pas un email valide dans le format local-part@hostname"

